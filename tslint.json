{
  "extends": [
    "@blueeast/tslint-config-blueeast/react"
  ],
  "rules": {
    "class-name":false,
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "max-line-length": [
      true,
      440
    ],
    "max-classes-per-file":false,
    "no-var-requires": false,
    "no-submodule-imports": false,
    "jsx-boolean-value": false,
    "no-console": false,
    "jsx-no-lambda": false,
    "no-shadowed-variable": false,
    "ter-indent": false,
    "sort-imports": false,
    "pascal-case": false
  }
}